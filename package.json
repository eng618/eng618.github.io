{
  "name": "engarcia",
  "private": true,
  "version": "1.28.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": "20.x"
  },
  "type": "module",
  "packageManager": "yarn@4.7.0",
  "scripts": {
    "build": "gatsby build",
    "build:clean": "yarn clean && gatsby build",
    "build:prod": "gatsby build --prefix-paths",
    "clean": "gatsby clean",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public -b master -m \"Auto-generated commit from manual deploy\"",
    "dev": "gatsby develop -H 0.0.0.0",
    "dev:clean": "gatsby clean && gatsby develop -H 0.0.0.0",
    "dev:reset": "yarn clean && rm -rf node_modules yarn.lock && yarn && yarn test && yarn dev",
    "format": "prettier --check \"src/**/*.{scss,css,js,jsx,md,mdx}\"",
    "format:fix": "prettier --write \"src/**/*.{scss,css,js,jsx,md,mdx}\"",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix --cache",
    "prepare": "husky install",
    "serve": "gatsby serve",
    "start": "yarn dev",
    "test": "yarn format && yarn lint"
  },
  "dependencies": {
    "@carbon/react": "^1.59.0",
    "@carbon/styles": "^1.77.2",
    "gatsby": "^5.13.6",
    "gatsby-plugin-image": "^3.13.1",
    "gatsby-plugin-manifest": "^5.14.0",
    "gatsby-plugin-offline": "^6.13.2",
    "gatsby-plugin-sharp": "^5.14.0",
    "gatsby-remark-copy-linked-files": "6.13.1",
    "gatsby-remark-images": "^7.13.1",
    "gatsby-source-filesystem": "^5.13.1",
    "gatsby-theme-carbon": "^4.0.1",
    "gatsby-transformer-remark": "^6.13.1",
    "gatsby-transformer-sharp": "^5.13.1",
    "lmdb": "^3.2.6",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "styled-components": "^6.1.16"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/eslint-parser": "^7.26.5",
    "@babel/eslint-plugin": "^7.25.9",
    "@babel/plugin-transform-react-jsx": "^7.25.9",
    "@eng618/prettier-config": "^2.2.14",
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
    "@types/react": "^18.3.3",
    "babel-loader": "^9.1.3",
    "eslint": "^9.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^8.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-webpack-plugin": "^4.2.0",
    "gatsby-plugin-eslint": "^4.0.4",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "webpack": "^5.98.0"
  },
  "resolutions": {
    "ansi-html": "^0.0.8",
    "cross-fetch": "^3.1.5",
    "glob-parent": "^6.0.1",
    "got": "^11.8.5",
    "node-fetch": "^2.6.7",
    "nth-check": "^2.0.1",
    "trim": "^0.0.3",
    "ws": "^7.4.6"
  },
  "lint-staged": {
    "src/**/*.(js|ts)?(x)": "eslint . --fix",
    "*.(js|md|ts)?(x)": "prettier --write",
    "*.(scss|css|json)": "prettier --write"
  },
  "prettier": "@eng618/prettier-config"
}
